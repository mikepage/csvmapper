{
  "version": "1.0",
  "inputDelimiter": ";",
  "outputDelimiter": ";",
  "decimalSeparator": ",",
  "mappings": {
    "id": "employee_id",
    "title": "gender",
    "name": "full_name",
    "department": "dept",
    "active": "is_active",
    "salary": "monthly_salary",
    "start_date": "hire_date"
  },
  "typeTransformations": {
    "id": "integer",
    "active": "boolean",
    "salary": "number"
  },
  "transformations": {
    "name": "uppercase",
    "department": "lowercase",
    "start_date": "date"
  },
  "valueConversions": {
    "title": {
      "Mr": "male",
      "Ms": "female",
      "Mrs": "female"
    }
  }
}
