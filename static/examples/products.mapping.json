{
  "version": "1.0",
  "inputDelimiter": ",",
  "outputDelimiter": ";",
  "decimalSeparator": ".",
  "mappings": {
    "sku": "product_code",
    "product_name": "name",
    "category": "category",
    "price": "unit_price",
    "stock": "quantity",
    "available": "in_stock",
    "last_updated": "updated_at"
  },
  "typeTransformations": {
    "price": "number",
    "stock": "integer",
    "available": "boolean"
  },
  "transformations": {
    "name": "trim",
    "category": "uppercase",
    "last_updated": "date"
  },
  "valueConversions": {
    "available": {
      "yes": "true",
      "no": "false"
    }
  }
}
